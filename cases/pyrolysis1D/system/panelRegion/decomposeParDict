FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 4;

method          structured;

structuredCoeffs
{
    method      simple;
    patches     (region0_to_panelRegion_panel);

    simpleCoeffs
    {
         n               ( 1 2 2 );
         delta           0.001;
    }
}


hierarchicalCoeffs
{
    n               ( 1 2 2 );
    delta           0.001;
    order           xyz;
}

manualCoeffs
{
    dataFile        "cellDecomposition";
}

metisCoeffs
{
